{
  "name": "crypto-date",
  "version": "1.0.0",
  "description": "crypto-date",
  "main": "index.js",
  "scripts": {
    "lint": "hardhat check && eslint ./test",
    "lint:fix": "eslint --fix --ext js,.jsx,.ts,.tsx .",
    "lint:fix-cache": "eslint --fix --cache --ext js,.jsx,.ts,.tsx .",
    "format": "hardhat format && prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "coverage": "hardhat coverage",
    "deploy:test": "npx hardhat run ./scripts/deploy.ts --network test",
    "deploy:main": "npx hardhat run ./scripts/deploy.ts --network main",
    "verify:test": "hardhat verify --network test 0x4a0ec98f29089633917d3e52e47bb48fc0979276"
  },
  "author": "Maksim Krukovich",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-solhint": "^3.0.1",
    "@openzeppelin/contracts": "^4.9.0",
    "@openzeppelin/contracts-upgradeable": "^4.9.0",
    "@openzeppelin/hardhat-upgrades": "^1.26.0",
    "@typechain/ethers-v5": "^11.0.0",
    "@typechain/hardhat": "^7.0.0",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "hardhat": "^2.14.0",
    "hardhat-deploy": "^0.11.29",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-prettier": "^0.0.1",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.4.1",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.2.0",
    "typescript": "*"
  },
  "dependencies": {}
}